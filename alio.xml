<?xml version="1.0"?>
<alio>

    <client>
        <file pattern="axxaxx">
            <io type="remote" />
            <addon type="debug" default="disable">
                <read mode="verbose" />
           </addon>
            <addon type="timer" table="yes" xml="yes" />
        </file>
        <file pattern="bxxbxx">
            <io type="standard" />
            <addon type="debug" default="disable">
                <write mode="verbose"/>
            </addon>
            <addon type="timer" />
        </file>
        <file pattern="eta_mean">
            <io type="standard" />
            <addon type="timer" xml="yes" />
        </file>
        <file pattern="ncrcat">
            <io type="standard" />
            <addon type="timer" xml="yes" />
        </file>
    </clients>

    <master>
    </master>

</alio>
